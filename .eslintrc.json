{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
          "no-console": "off",
          "consistent-return": "off",
          "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
     ]
    }
}
